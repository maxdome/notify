{
  "name": "@maxdome/notify",
  "version": "0.5.2",
  "description": "Send CD / CI notifications from GitLab CI",
  "main": "src/cli.js",
  "scripts": {
    "fmt": "maxdome-prettier",
    "test": "NODE_ENV=test mocha './test/**/*.spec.js'",
    "test:coverage": "nyc npm test"
  },
  "bin": {
    "notify": "bin/notify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxdome/notify.git"
  },
  "author": "Chris Neuh√§user <chris@type10.com> (http://type10.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxdome/notify/issues"
  },
  "homepage": "https://github.com/maxdome/notify#readme",
  "engines": {
    "node": ">= 6"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^2.12.2",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^2.1.1"
  },
  "devDependencies": {
    "@maxdome/prettier": "^1.3.2",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "mock-require": "^2.0.2",
    "nyc": "^11.6.0",
    "sinon": "^4.2.0",
    "sinon-chai": "^2.14.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
